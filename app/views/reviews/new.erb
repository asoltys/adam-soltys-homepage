<h1>Add Review</h1>

<% form_for(@review) do |f| %>
	<p>
		<label for="review_title">Title:</label>
		<%= f.text_field :title %>
	</p>
		
	<p>
		<label for="review_rating">Rating:</label>
		<%= f.text_field :rating %>
	</p>
	
	<p>
		<label for="review_comments">Comments:</label>
		<%= f.text_field :comments %>
	</p>
	
	<%= submit_tag %>
<% end %>
	
	
	